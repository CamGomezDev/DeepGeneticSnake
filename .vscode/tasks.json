{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "Run Sketch",
      "type": "shell",
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "command": "${config:processing.path}",
      "presentation": {
        "echo": true,
        "reveal": "always",
        "focus": false,
        "panel": "dedicated"
      },
      "args": [
    "--force",
		"--sketch=C:/Users/User/Documents/Processing/stuff/gensnake",
		"--output=C:/Users/User/Documents/Processing/stuff/gensnake/out",
		"--run"
	  ],
      "windows": {
        "args": [
          "--force",
          "--sketch=C:/Users/User/Documents/Processing/stuff/gensnake",
          "--output=C:/Users/User/Documents/Processing/stuff/gensnake/out",
          "--run"
        ]
      }
    }
  ]
}